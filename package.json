{
  "name": "@heroku-cli/plugin-status",
  "description": "CLI Plugin for Heroku Status",
  "version": "5.0.4",
  "author": "Jeff Dickey @jdxcode",
  "anycli": {
    "commands": "./lib/commands",
    "topics": {
      "name": "status",
      "description": "status of the Heroku platform"
    },
    "devPlugins": [
      "@anycli/plugin-help"
    ]
  },
  "bugs": {
    "url": "https://github.com/heroku/heroku-cli-status/issues"
  },
  "cli-engine": {
    "topics": {
      "status": {
        "description": "status of the Heroku platform"
      }
    },
    "commands": "./lib/commands"
  },
  "dependencies": {
    "@anycli/command": "^1.2.10",
    "@anycli/config": "^1.3.14",
    "@anycli/errors": "^0.2.1",
    "@fancy-test/nock": "^0.0.1",
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^8.0.0-anycli.1",
    "cli-ux": "^3.3.18",
    "date-fns": "^1.29.0",
    "http-call": "^5.0.2"
  },
  "devDependencies": {
    "@anycli/dev-cli": "^0.2.2",
    "@anycli/plugin-help": "^0.6.5",
    "@anycli/test": "^0.10.11",
    "@anycli/tslint": "^0.2.7",
    "@heroku-cli/tslint": "1.1.4",
    "@types/ansi-styles": "^2.0.30",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/nock": "^9.1.2",
    "@types/node": "^9.4.0",
    "@types/supports-color": "^3.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "globby": "^7.1.1",
    "mocha": "^5.0.0",
    "nock": "^9.1.6",
    "ts-node": "^4.1.0",
    "tslint": "5.9.1",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".anycli.manifest.json",
    "lib"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-status",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "ISC",
  "repository": "heroku/heroku-cli-status",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"tsc -p test --noEmit\" \"tslint -p test -t stylish\"",
    "postpublish": "rm .anycli.manifest.json",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && anycli-dev manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  }
}
